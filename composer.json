{
    "name": "adriandmitroca/quickselect",
    "description": "PHP project.",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Dmitroca",
            "email": "dmitroca.adrian@gmail.com"
        }
