    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "AdrianDmitroca\\QuickSelect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdrianDmitroca\\QuickSelect\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit",
        "configure-commit-template": "git config --add commit.template .gitmessage",
        "cs-fix": "php-cs-fixer fix . -vv || true"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "friendsofphp/php-cs-fixer": "^1.12"
    }
}
